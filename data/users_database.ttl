@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix foo: <http://example.com/owl/foodology#>.

foo:peter_grant
    a foaf:Person, foaf:Person;
    foo:allergy_eggs "yes";
    foo:allergy_fish "no";
    foo:allergy_milk "yes";
    foo:allergy_peanuts "no";
    foo:allergy_shellfish "yes";
