@prefix : <http://example.com/owl/foodology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dbc: <http://dbpedia.org/resource/Category:> .
@prefix dbr: <http://dbpedia.org/resource/> .

## Properties

:id a owl:ObjectProperty .

:name a owl:ObjectProperty .

:has_stove a owl:ObjectProperty;
         rdfs:domain dbc:Cooking_appliances ;
         rdfs:range dbr:Stove .

:has_oven a owl:ObjectProperty;
         rdfs:domain dbc:Cooking_appliances ;
         rdfs:range dbr:Oven .

:has_pot a owl:ObjectProperty;
         rdfs:domain dbc:Cooking_appliances ;
         rdfs:range dbr:Pot .

:has_pan a owl:ObjectProperty;
         rdfs:domain dbc:Cooking_appliances ;
         rdfs:range dbr:Pan .

:has_kettle a owl:ObjectProperty;
         rdfs:domain dbc:Cooking_appliances ;
         rdfs:range dbr:Kettle .

## Users

:jane_doe a foaf:Person;
    :id "jane_doe";
    :name "Jane Doe";
    :has_stove "yes";
    :has_oven "no";
    :has_pot "yes";
    :has_pan "yes";
    :has_kettle "no" .
