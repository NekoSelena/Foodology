foo:has_kettle
    a owl:ObjectProperty;
    rdfs:domain <http://dbpedia.org/resource/Category:Cooking_appliances>;
    rdfs:range dbr:Kettle.
foo:has_oven
    a owl:ObjectProperty;
    rdfs:domain <http://dbpedia.org/resource/Category:Cooking_appliances>;
    rdfs:range dbr:Oven.
foo:has_pan
    a owl:ObjectProperty;
    rdfs:domain <http://dbpedia.org/resource/Category:Cooking_appliances>;
    rdfs:range dbr:Pan.
foo:has_pot
    a owl:ObjectProperty;
    rdfs:domain <http://dbpedia.org/resource/Category:Cooking_appliances>;
    rdfs:range dbr:Pot.
foo:has_stove
    a owl:ObjectProperty;
    rdfs:domain <http://dbpedia.org/resource/Category:Cooking_appliances>;
    rdfs:range dbr:Stove.
foo:id a owl:ObjectProperty.

