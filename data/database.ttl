@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix dbr: <http://dbpedia.org/resource/>.
@prefix foo: <http://example.com/owl/foodology#>.

foo:alexander_flemming
    a foaf:Person;
    foo:has_kettle "yes";
    foo:has_oven "no";
    foo:has_pan "yes";
    foo:has_pot "no";
    foo:has_stove "yes";
    foo:id "alexander_flemming";
    foo:name "Alexander Flemming".
foo:has_kettle
    a owl:ObjectProperty;
    rdfs:domain <http://dbpedia.org/resource/Category:Cooking_appliances>;
    rdfs:range dbr:Kettle.
foo:has_oven
    a owl:ObjectProperty;
    rdfs:domain <http://dbpedia.org/resource/Category:Cooking_appliances>;
    rdfs:range dbr:Oven.
foo:has_pan
    a owl:ObjectProperty;
    rdfs:domain <http://dbpedia.org/resource/Category:Cooking_appliances>;
    rdfs:range dbr:Pan.
foo:has_pot
    a owl:ObjectProperty;
    rdfs:domain <http://dbpedia.org/resource/Category:Cooking_appliances>;
    rdfs:range dbr:Pot.
foo:has_stove
    a owl:ObjectProperty;
    rdfs:domain <http://dbpedia.org/resource/Category:Cooking_appliances>;
    rdfs:range dbr:Stove.
foo:id a owl:ObjectProperty.

